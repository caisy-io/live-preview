{
  "name": "@nicolasshiken/live-preview-javascript",
  "version": "1.0.91",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "author": "nicolasshiken <nicolasshiken@gmail.com>",
  "exports": {
    "./styles": "./src/inspect/styles.css",
    "./indicatorStyles": "./src/caisy-connection-indicator/styles.css",
    ".": "./dist/index.js",
    "./getCaisyInspectProps": "./dist/getCaisyInspectProps.js"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build-index": "tsup src/index.ts --format cjs,esm --dts",
    "build-inspect-props": "tsup src/getCaisyInspectProps.ts --format cjs,esm --dts",
    "build": "npm run build-index && npm run build-inspect-props"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@turbo/gen": "^1.11.3",
    "@types/eslint": "^8.56.1",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "getCaisyInspectProps": [
        "dist/getCaisyInspectProps.d.ts"
      ]
    }
  }
}
